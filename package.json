{
  "name": "nuxt-neo",
  "version": "3.0.2",
  "description": "A nuxt.js module that implements a universal api layer, same-way compatible between server and client side.",
  "keywords": [
    "nuxt",
    "vue",
    "node",
    "javascript",
    "module",
    "api",
    "middleware",
    "service",
    "universal",
    "neo",
    "controller",
    "action",
    "asyncdata",
    "fetch"
  ],
  "homepage": "https://ezypeeze.github.io/nuxt-neo",
  "repository": {
    "type": "git",
    "url": "https://github.com/ezypeeze/nuxt-neo"
  },
  "bugs": {
    "url": "https://github.com/ezypeeze/nuxt-neo/issues"
  },
  "main": "lib/module.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "ava",
    "lint": "eslint --ext .js lib",
    "release:patch": "sure && standard-version --no-verify --release-as patch && git push --tags",
    "release:minor": "sure && standard-version --no-verify --release-as minor && git push --tags",
    "release:major": "sure && standard-version --no-verify --release-as major && git push --tags",
    "nyc:ava": "nyc ava"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Pedro Pereira <pedromdspereira.93@gmail.com>",
  "contributors": [
    {
      "name": "Pedro Pereira <pedromdspereira.93@gmail.com>"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.10",
    "nuxt": "~2.8.1",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "querystring": "^0.2.0",
    "standard-version": "^6.0.1",
    "sure": "^0.0.3",
    "vuepress": "^1.0.2"
  },
  "ava": {
    "cache": true,
    "concurrency": 1,
    "failFast": true,
    "failWithoutAssertions": false,
    "verbose": true,
    "require": [
      "./tests/setup/global.js"
    ],
    "files": [
      "tests/*.test.js"
    ]
  }
}
